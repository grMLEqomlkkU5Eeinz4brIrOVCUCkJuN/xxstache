{
	"name": "xxstache",
	"version": "1.0.0",
	"description": "A port of hybrid-disk-cache using xxhash64 for toy projects",
	"main": "dist/index.js",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist/**/*",
		"README.md",
		"docs.md",
		"LICENSE"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.js"
		}
	},
	"engines": {
		"node": ">=14"
	},
	"scripts": {
		"build": "tsc",
		"test": "jest",
		"bench": "ts-node benchmarks/bench.ts",
		"lint": "eslint .",
		"clean": "rimraf dist .tmp",
		"ci": "npm run lint && npm run test && npm run build",
		"prepublishOnly": "npm run build"
	},
	"keywords": [
		"cache",
		"disk-cache",
		"sqlite",
		"hybrid",
		"ttl",
		"lru",
		"xxhash"
	],
	"author": "",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/grMLEqomlkkU5Eeinz4brIrOVCUCkJuN/xxstache.git"
	},
	"bugs": {
		"url": "https://github.com/grMLEqomlkkU5Eeinz4brIrOVCUCkJuN/xxstache/issues"
	},
	"homepage": "https://github.com/grMLEqomlkkU5Eeinz4brIrOVCUCkJuN/xxstache#readme",
	"devDependencies": {
		"@types/better-sqlite3": "^7.6.13",
		"@types/fs-extra": "^11.0.4",
		"@types/jest": "^30.0.0",
		"@types/node": "^24.9.1",
		"@types/xxhashjs": "^0.2.4",
		"eslint": "^9.38.0",
		"jest": "^30.2.0",
		"rimraf": "^6.0.1",
		"ts-jest": "^29.4.5",
		"ts-node": "^10.9.2",
		"typescript-eslint": "^8.46.2"
	},
	"dependencies": {
		"better-sqlite3": "^12.4.1",
		"fs-extra": "^11.3.2",
		"xxhash-wasm": "^1.1.0"
	}
}
